<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://axelor.com/xml/ns/object-views
       http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
      
   <grid model="com.axelor.employee.db.EmployeeInfo" title="EmployeeInfos" name="employeeInfo-grid">
      <field name="name" />
     <field name="dateOfBirth"/>
     <field name="timeOfBirth"/>
     <field name="dateAndTime"/>
   </grid>
      
    <form model="com.axelor.employee.db.EmployeeInfo" title="EmployeeInfo" name="employeeInfo-form">
      <panel>
          <field name="name" />
          <field name="dateOfBirth"/>
          <field name="timeOfBirth"/>
          <field name="dateAndTime"/>
          <field name="sequence"/>
          <field name="password"/>
           <field name="image" />
      </panel>    
    </form>  
       
  </object-views>     